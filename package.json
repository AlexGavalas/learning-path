{
    "name": "learning-path",
    "version": "0.3.0",
    "type": "module",
    "private": true,
    "engines": {
        "node": "18"
    },
    "scripts": {
        "dev": "astro dev",
        "start:server": "node ./dist/server/entry.mjs",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro",
        "index:notes": "tsm scripts/index-notes.ts",
        "index:summaries": "tsm scripts/index-lesson-summaries.runner.ts",
        "sync:types:supabase": "supabase gen types typescript --project-id cxftalfhgyhwkiwnyqjh --schema public > src/types/database.types.ts && pnpm format",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
        "test:watch": "jest --watch",
        "lint": "eslint --ignore-path .gitignore .",
        "prettier": "prettier .",
        "check:format": "pnpm prettier --list-different",
        "check:types": "tsc --noEmit",
        "format": "pnpm prettier --write",
        "prepare": "husky install",
        "validate": "pnpm lint && pnpm check:format && pnpm check:types"
    },
    "dependencies": {
        "@astrojs/mdx": "0.19.7",
        "@astrojs/node": "5.2.0",
        "@astrojs/react": "2.2.1",
        "@astrojs/sitemap": "1.3.3",
        "@astrojs/tailwind": "3.1.3",
        "@astrojs/vercel": "3.5.0",
        "@supabase/supabase-js": "2.25.0",
        "@vercel/edge-config": "0.2.0",
        "astro": "2.6.4",
        "date-fns": "2.30.0",
        "gray-matter": "4.0.3",
        "next": "13.4.5",
        "next-mdx-remote": "4.4.1",
        "next-themes": "0.2.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "rehype-autolink-headings": "6.1.1",
        "rehype-external-links": "2.1.0",
        "rehype-slug": "5.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.3",
        "@commitlint/config-conventional": "17.6.3",
        "@tailwindcss/typography": "0.5.9",
        "@testing-library/dom": "9.3.1",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@testing-library/user-event": "14.4.3",
        "@trivago/prettier-plugin-sort-imports": "4.1.1",
        "@types/jest": "29.5.1",
        "@types/jest-axe": "3.5.5",
        "@types/node": "18.16.9",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.5",
        "@typescript-eslint/eslint-plugin": "5.59.7",
        "@typescript-eslint/parser": "5.59.7",
        "axios": "1.4.0",
        "date-fns-tz": "2.0.0",
        "dotenv": "16.3.0",
        "encoding": "0.1.13",
        "eslint": "8.42.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-config-standard-with-typescript": "35.0.0",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-n": "15.0.0",
        "eslint-plugin-promise": "6.0.0",
        "eslint-plugin-react": "7.32.2",
        "husky": "8.0.3",
        "jest": "29.5.0",
        "jest-axe": "7.0.1",
        "jest-environment-jsdom": "29.5.0",
        "lint-staged": "13.2.2",
        "prettier": "2.8.8",
        "prettier-plugin-astro": "0.10.0",
        "prettier-plugin-tailwindcss": "0.3.0",
        "react-markdown": "8.0.7",
        "supabase": "1.68.6",
        "tailwindcss": "3.3.2",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.1",
        "tsm": "2.3.0",
        "typescript": "5.0.4",
        "unified": "10.1.2"
    }
